t[#segment]\n        if foodX == segment_x then\n          if foodY == segment_y then\n            possible = false\n          end\n        end\n\n        -- If possible insert position to array\n        if possible then\n          table.insert(possibleFoodPositions, #possibleFoodPositions + 1, {foodX, foodY})\n        end\n      end\n    end\n  end\n\n  -- Take random position from array of positions\n  foodPosition = possibleFoodPositions[love.math.random(1, (#possibleFoodPositions))]\nend\n"},{"blockly":null,"code":"function list_get_last(t)\n  return t[#t]\nend\n\nfunction love.update( dt )\n  -- Move snake every 0.15s\n  timer = timer + dt\n  if snakeAlive then\n    if timer >= 0.15 then\n      timer = 0\n      canMove = true\n\n      -- Pop first item from Queue -> if only one item move in that direction\n      if #directionQueue > 1 then\n        table.remove(directionQueue, 1)\n      end\n\n      -- Add new head position\n      nextXPosition = (snakeSegments[1])[1]\n      nextYPosition = list_get_last((snakeSegments[1]))\n\n      -- Move in every direction\n      if directionQueue[1] == 'right' then\n        nextXPosition = nextXPosition + 1\n        -- Check layout size and snake position\n        if nextXPosition > gridXCount then\n          nextXPosition = 1\n        end\n      elseif directionQueue[1] == 'left' then\n        nextXPosition = nextXPosition - 1\n        if nextXPosition < 1 then\n          nextXPosition = gridXCount\n        end\n      elseif directionQueue[1] == 'down' then\n        nextYPosition = nextYPosition + 1\n        if nextYPosition > gridYCount then\n          nextYPosition = 1\n        end\n      elseif directionQueue[1] == 'up' then\n        nextYPosition = nextYPosition - 1\n        if nextYPosition < 1 then\n          nextYPosition = gridYCount\n        end\n      end\n    end\n  end\nend"},{"blockly":null,"code":"if canMove then\n-- Add new head position\ntable.insert(snakeSegments, 1, {nextXPosition, nextYPosition})\n\n-- If food == head dont remove tail (snake has eaten food)\nif (snakeSegments[1])[1] == foodPosition[1] then\n  if list_get_last((snakeSegments[1])) == foodPosition[#foodPosition] then\n    -- Create new food\n    createFood()\n  else\n    -- Remove tail (automatically removes last element)\n    table.remove(snakeSegments, (#snakeSegments))\n  end\nelse\n  table.remove(snakeSegments, (#snakeSegments))\nend\nelse\n  snakeAlive = false\nend"},{"blockly":null,"code":"-- Check if game is over\nfor segmentIndex, segment in ipairs(snakeSegments) do\n  if segmentIndex ~= #snakeSegments then\n    if nextXPosition == segment[1] then\n      if nextYPosition == segment[#segment] then\n        canMove = false\n      end\n    end\n  end\nend"},{"blockly":null,"code":"function love.keypressed(key)\n    -- Keypress -> add to Queue -> check if != last direction and != opposite direction\n    if key == 'right' and directionQueue[#directionQueue] ~= 'left' and directionQueue[#directionQueue] ~= 'right' then\n        table.insert(directionQueue, 'right')\n    elseif key == 'left' and directionQueue[#directionQueue] ~= 'right' and directionQueue[#directionQueue] ~= 'left' then\n        table.insert(directionQueue, 'left')\n    elseif key == 'up' and directionQueue[#directionQueue] ~= 'down' and directionQueue[#directionQueue] ~= 'down' then\n        table.insert(directionQueue, 'up')\n    elseif key == 'down' and directionQueue[#directionQueue] ~= 'up' and directionQueue[#directionQueue] ~= 'up' then\n        table.insert(directionQueue, 'down')\n    end\nend"},{"blockly":null,"code":"function love.load()\n    -- touch controls\n    begin_y = nil\n    end_y = nil\n    begin_x = nil\n    end_x = nil\n    doingTouch = false\n\n    -- Create layout\n    cellSize = 20\n    screen_height = love.graphics.getHeight()\n    screen_width = love.graphics.getWidth()\n    gridYCount = (screen_height - screen_height % 20) / 20\n    gridXCount = (screen_width - screen_width % 20 ) / 20\n\n    reset_game()\nend\n\nfunction love.mousepressed(x, y, button)\n    if doingTouch == false then\n        begin_y = y\n        begin_x = x\n        doingTouch = true\n    end\nend\n\nfunction love.mousereleased(x, y, button)\n    end_y = y\n    end_x = x\n    dx = math.abs(begin_x - end_x)\n    dy = math.abs(begin_y - end_y)\n    print(begin_x, end_x, dx)\n    print(begin_y, end_y, dy)\n    if doingTouch == true then\n        if dx < dy then\n            if end_y < begin_y then\n                --do what you want if you swiped up\n                print('up')\n                if directionQueue[#directionQueue] ~= 'down' and directionQueue[#directionQueue] ~= 'down'then\n                    table.insert(directionQueue, 'up')\n                end\n            elseif end_y > begin_y then\n                --do what you want if you swiped down\n                print('down')\n                if directionQueue[#directionQueue] ~= 'up' and directionQueue[#directionQueue] ~= 'up' then\n                    table.insert(directionQueue, 'down')\n                end\n            end\n        elseif dx > dy then\n            if end_x > begin_x then\n                --do what you want if you swiped left\n                print('left')\n                if directionQueue[#directionQueue] ~= 'left' and directionQueue[#directionQueue] ~= 'right' then\n                    table.insert(directionQueue, 'right')\n                end\n            elseif end_x < begin_x then\n                --do what you want if you swiped right\n                print('right')\n                if directionQueue[#directionQueue] ~= 'right' and directionQueue[#directionQueue] ~= 'left' then\n                    table.insert(directionQueue, 'left')\n                end\n            end\n        else\n            --do what you want if you touch\n            if snakeAlive == false then\n                reset_game()\n            end\n        end\n\n        begin_y = nil\n        begin_x = nil\n        end_y = nil\n        end_x = nil\n        doingTouch = false\n    end\nend\n"}]}